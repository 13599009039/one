function loadDataFromStorage() {
    const savedData = localStorage.getItem('ajkuaiji_data');
    if (savedData) {
        try {
            return JSON.parse(savedData);
        } catch (e) {
            console.error('解析localStorage数据失败:', e);
            return getDefaultData();
        }
    }
    return getDefaultData();
}

